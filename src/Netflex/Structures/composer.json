{
    "name": "netflex/structures",
    "description": "Netflex Structure library",
    "type": "library",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Netflex\\Structure\\": ""
        }
    },
    "replace": {
        "netflex/structure": "*"
    },
    "require": {
        "apility/rule-builder": "^1.1",
        "haydenpierce/class-finder": "^0.4.3",
        "netflex/pages": "v4.26.0",
        "netflex/query-builder": "v4.26.0",
        "netflex/support": "v4.26.0",
        "php": "^7.4|^8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.25.x-dev"
        },
        "laravel": {
            "providers": [
                "Netflex\\Structure\\Providers\\StructureServiceProvider"
            ]
        }
    }
}
